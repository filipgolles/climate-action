{
  "name": "climate-action",
  "version": "0.1.0",
  "description": "Enabling climate-friendly behavior through Actions on Google",
  "main": "lib/core/webhook.js",
  "scripts": {
    "newman": "newman run test/http/reqs.json -e test/http/env.json -r cli",
    "test": "tape -r dotenv/config test/**/*.test.js | tap-dot",
    "coverage": "nyc npm run test",
    "agent":
      "./agent/gactions update --action_package ./agent/action.json --project climate-actions-87369",
    "train": "node lib/classifier/train.js",
    "tunnel": "ngrok start -config ~/.ngrok2/ngrok.yml climate",
    "serve": "node -r dotenv/config lib/core/webhook.js",
    "start": "npm run tunnel & npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipgolles/climate-action.git"
  },
  "author": "Filip Norman Golles (https://github.com/filipgolles)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filipgolles/climate-action/issues"
  },
  "homepage": "https://github.com/filipgolles/climate-action#readme",
  "nyc": {
    "reporter": "text"
  },
  "dependencies": {
    "actions-on-google": "^1.10.0",
    "express": "^4.16.3",
    "got": "^8.3.0",
    "moment": "^2.21.0",
    "morgan": "^1.9.0",
    "natural": "^0.5.6"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "eslint": "^4.19.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.9.0",
    "newman": "^3.9.3",
    "nyc": "^11.6.0",
    "tap-dot": "^1.0.5",
    "tape": "^4.9.0"
  }
}
