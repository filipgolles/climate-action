{
  "name": "climate-action",
  "version": "0.1.0",
  "description": "Enabling climate-friendly behavior through Actions on Google",
  "main": "build/init.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "babel src -d build -D",
    "build:production": "cross-env BABEL_ENV=production babel src -d dist -D",
    "prestart": "npm run build",
    "start": "node build/init.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filipgolles/climate-action.git"
  },
  "author": "Filip Norman Golles <filip.golles@gmail.com> (https://github.com/filipgolles)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filipgolles/climate-action/issues"
  },
  "homepage": "https://github.com/filipgolles/climate-action#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.1",
    "eslint-config-skellyton": "^1.0.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-promise": "^3.6.0"
  },
  "eslintConfig": {
    "extends": [
      "skellyton"
    ]
  },
  "eslintIgnore": [
    "build/",
    "dist/"
  ],
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ],
    "plugins": [],
    "comments": false,
    "env": {
      "production": {
        "plugins": [
          "transform-remove-console"
        ]
      }
    }
  },
  "dependencies": {
    "tape": "^4.9.0"
  }
}
